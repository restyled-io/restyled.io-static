#!/usr/bin/env bash
set -euo pipefail
export AWS_PROFILE=restyled

{
  echo dev
  echo stable
  aws s3 ls s3://sites-docs-bucket-1vleclrg7tli7/data-files/restylers/manifests/ |
    awk '{print $2}' |
    sed '/^\(v.*\)\/$/!d; s//\1/' |
    sort -rV
} |
  while read -r channel; do
    printf '%-60.60s %s\n' \
      "/data-files/restylers/manifests/$channel/restylers.yaml" \
      "https://github.com/restyled-io/restylers/releases/download/$channel/restylers.yaml"
  done
